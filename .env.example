# Local LLM or OpenAI-compatible endpoint
LLM_BASE_URL=http://localhost:11434/v1
LLM_API_KEY=
LLM_MODEL=qwen2.5:7b-instruct
TEMPERATURE=0.0

# Planning / ToT
TOT_GATE=1
TOT_BRANCHES=3
PLANNER_CONFIDENCE_THRESHOLD=0.6

# Executor
EXEC_MAX_ITERS=6
REFLEXION=1
REFLEXION_MAX_RETRIES=1

# HITL
REQUIRE_PLAN_APPROVAL=1

# LangSmith Tracing (optional)
LANGCHAIN_TRACING_V2=false
LANGSMITH_TRACING=false
LANGSMITH_API_KEY=
LANGSMITH_ENDPOINT=

# LangChain (optional)
LANGCHAIN_API_KEY=
LANGCHAIN_ENDPOINT=
